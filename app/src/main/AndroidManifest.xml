<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:authorities="爸爸们"
        android:theme="@style/Theme.WordsLearning"
        android:usesCleartextTraffic="true">

        <!--        用户登录窗口-->
        <activity android:name=".UserView.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.LoginActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!--        用户注册窗口-->
        <activity android:name=".UserView.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.RegisterActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!--        找回密码窗口-->
        <activity android:name=".UserView.FindPwActivity">
            <intent-filter>
                <action android:name="android.intent.action.RegisterActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!--        修改密码窗口-->
        <activity android:name=".UserView.ChangePwActivity">
            <intent-filter>
                <action android:name="android.intent.action.ChangePwActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity android:name=".MainView.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--        生词本-->
        <activity android:name=".MainView.Personal.utilsActivity.DiffCollectionActivity">
            <intent-filter>
                <action android:name="android.intent.action.DiffCollectionActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--        错词本-->
        <activity android:name=".MainView.Personal.utilsActivity.WrongColeectionActivity">
            <intent-filter>
                <action android:name="android.intent.action.WrongColeectionActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--        学习数据窗口-->
        <activity android:name=".MainView.Personal.utilsActivity.LearnDataActivity">
            <intent-filter>
                <action android:name="android.intent.action.LearnDataActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--        关于我们窗口-->
        <activity android:name=".MainView.Personal.utilsActivity.AboutUsActivity">
            <intent-filter>
                <action android:name="android.intent.action.AboutUsActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--        用户协议窗口-->
        <activity android:name=".MainView.Personal.utilsActivity.UserAgreementActivity">
            <intent-filter>
                <action android:name="android.intent.action.UserAgreementActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <!--        学习设置窗口-->
        <activity android:name=".MainView.Personal.utilsActivity.LearnOptionActivity">
            <intent-filter>
                <action android:name="android.intent.action.LearnOptionActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--        学习设置窗口-->
        <activity android:name=".MainView.Personal.utilsActivity.PreferenceActivity">
            <intent-filter>
                <action android:name="android.intent.action.PreferenceActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--        学习计划窗口-->
        <activity android:name=".MainView.Personal.utilsActivity.LearnPlanActivity">
            <intent-filter>
                <action android:name="android.intent.action.LearnPlanActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainView.LearnWords.WordsRemember">
            <intent-filter>
                <action android:name="android.intent.action.WordsRemember" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>



    </application>
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>